{
  "bootstrap": [
    {
      "name":"rsp logger",
      "interfaces":[
        "rsp_logger.impl.SimpleMetricLogger"
      ],
      "factory": {
        "impl" :"rsp_logger.logger_factory.LoggerFactory",
        "getter":"getFactory",
        "params":{
          "positional": ["config/logger/${meta.Environment}/app_config.json"]
        }
      }
    },
    {
      "name":"rsp store",
      "interfaces":[
        "rsp.types.providers.model_storage_provider.ModelStorageProvider"
      ],
      "factory": {
        "impl" :"rsp_store.model.ModelStoreFactory",
        "params":{
          "positional": ["config/model_store/local/model_store.json","config/model_store/local/model_serder.json"]
        },
        "getter":{
          "params":{
            "positional": ["local_rw"]
          }
        }
      }
    }
  ]
}