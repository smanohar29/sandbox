{
  "logger": {
    "impl": "rsp_logger.impl.metric_logger.SimpleMetricLogger",
    "level": "INFO",
    "metadataLocation": "config/logger/prod/app_metadata.json",
    "params": {
      "format": "%(asctime)s %(levelname)s %(name)s.%(funcName)s:%(lineno)d %(message)s",
      "app_id": "sample_id",
      "default_app_id": 0
    }
  },
  "handlers": {
    "default": [
            {
        "impl": "rsp_logger.handler.file_system_handler.FileSystemHandler",
        "params": {
          "kwargs":{
            "dir": "/apps/logs/202720/prod/",
            "filename": "app.log"
          }
        }
      }
    ]
  }
}